<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Single Login Test" verbose="1">
    
    <listeners>
        <listener class-name="com.enterprise.mercury.core.listeners.TestListener"/>
    </listeners>
    
    <test name="Login Test">
        <classes>
            <class name="com.enterprise.mercury.ui.tests.LoginTest">
                <methods>
                    <include name="testSuccessfulLogin"/>
                </methods>
            </class>
        </classes>
    </test>
    
</suite>
